{
	"name": "generator-wp-dev-env",
	"version": "0.5.0",
	"subModules": {
		"wp-dev-env-grunt": "0.6.0",
		"croox/wp-dev-env-frame": "0.2.5"
	},
	"description": "Yeoman generator for scaffolding Grunt based WordPress Plugins/Themes/Childthemes development environment",
	"main": "generators/index.js",
	"homepage": "https://github.com/croox/generator-wp-dev-env",
	"author": {
		"name": "jhotadhari",
		"email": "tellme@waterproof-webdesign.info",
		"url": "https://github.com/jhotadhari"
	},
	"files": [
		"generators",
		"utils"
	],
	"keywords": [
		"WordPress",
		"wp",
		"theme",
		"plugin",
		"wp-dev-env",
		"grunt",
		"yeoman-generator"
	],
	"devDependencies": {
		"eslint": "^5.9.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-config-xo": "^0.25.0",
		"eslint-plugin-prettier": "^3.0.0",
		"husky": "^1.2.0",
		"jest": "^23.5.0",
		"lint-staged": "^8.1.0",
		"prettier": "^1.15.2",
		"yeoman-assert": "^3.1.0",
		"yeoman-test": "^1.7.0"
	},
	"engines": {
		"npm": ">= 4.0.0"
	},
	"dependencies": {
		"chalk": "^2.1.0",
		"childprocess": "^2.0.2",
		"enquirer": "^2.3.0",
		"figlet": "^1.2.1",
		"gradient-string": "^1.2.0",
		"yeoman-generator": "^2.0.1",
		"yosay": "^2.0.1"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"prettier"
		],
		"env": {
			"jest": true,
			"node": true
		},
		"rules": {
			"prettier/prettier": "error"
		},
		"plugins": [
			"prettier"
		]
	},
	"scripts": {
		"pretest": "eslint .",
		"test": "jest"
	},
	"repository": "croox/generator-wp-dev-env",
	"license": "GPL-3.0"
}
