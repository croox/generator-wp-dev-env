{
  "name": "generator-wp-dev-env",
  "version": "0.0.0",
  "description": "Yeoman generator for scaffolding a Grunt based WordPress Plugin & Theme dev environment ",
  "homepage": "https://github.com/jhotadhari/generator-wp-dev-env",
  "author": {
    "name": "jhotadhari",
    "email": "tellme@waterproof-webdesign.info",
    "url": "waterproof-webdesign.info"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "WordPress",
    "theme",
    "plugin",
    "grunt",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "jhotadhari/generator-wp-dev-env",
  "license": "GPL-3.0"
}
